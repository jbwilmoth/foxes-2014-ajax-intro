<table>
  <thead>
    <tr>
      <th>First Name</th>
      <th>Last Name</th>
      <th>Email</th>
      <th>Phone</th>
    </tr>
  </thead>
  <tbody>
    <% @contacts.each do |c| %>
      <%= erb :_contact_row, layout: false, locals: {contact: c} %>
    <% end %>
  </tbody>
</table>
<a id="new-contact" href="/contacts/new" title="New Contact">New Contact</a>
